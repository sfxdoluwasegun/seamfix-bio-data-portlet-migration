<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN"
        "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>

    <portlet>

        <portlet-name>biodata-portlet</portlet-name>
        <configuration-action-class>com.liferay.portal.kernel.portlet.DefaultConfigurationAction
        </configuration-action-class>

        <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper
        </friendly-url-mapper-class>
        <friendly-url-mapping>biodata</friendly-url-mapping>
        <friendly-url-routes>friendly-url-router.xml</friendly-url-routes>

        <preferences-company-wide>true</preferences-company-wide>
        <instanceable>false</instanceable>

        <requires-namespaced-parameters>false</requires-namespaced-parameters>

        <header-portlet-css>/static/css/bootstrap.min.css</header-portlet-css>
        <header-portlet-css>/static/css/main.css</header-portlet-css>

        <footer-portlet-javascript>/static/js/jquery/jquery-1.11.1.min.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/bootstrap.min.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/angular/angular.min-1.5.5.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/angular/ui-bootstrap-tpls-1.3.2.min.js</footer-portlet-javascript>


        <footer-portlet-javascript>/static/js/underscore-min.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/html2canvas.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/pdfmake.min.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/angular/app-init.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/angular/controller.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/angular/service.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/angular/factory.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/angular/app-bootstrap.js</footer-portlet-javascript>
        <footer-portlet-javascript>/static/js/jquery/app.js</footer-portlet-javascript>
    </portlet>

</liferay-portlet-app>